{
  "extends": [ "./node_modules/grommet-toolbox/.eslintrc" ],
  "plugins": [ "import" ],
  "rules":  {
    "no-undef": 2,
    "prefer-const": 2,
    "no-redeclare": 2,
    "no-trailing-spaces": 2,
    "no-unreachable": 2,
    "react/jsx-no-undef": 2,
    "import/no-unresolved": 2,
    "import/named": 2,
    "import/namespace": 2,
    "import/default": 2,
    "import/export": 2
  }
}
